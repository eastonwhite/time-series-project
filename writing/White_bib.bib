keywords = {Adaptation, Physiological,Animals,Ants,Ants: genetics,Fungi,Fungi: genetics,Genes, Fungal,Genome,Insect Proteins,Insect Proteins: genetics,Insect Proteins: metabolism,Male,Molecular Sequence Data,Phylogeny,Sexual Behavior, Animal,Symbiosis},
