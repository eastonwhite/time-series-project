file = {:Users/eastonwhite/Library/Application Support/Mendeley Desktop/Downloaded/Sokolow et al. - 2009 - Disease dynamics in marine metapopulations Modelling infectious diseases on coral reefs.pdf:pdf},
isbn = {1365-2664},
issn = {00218901},
journal = {Journal of Applied Ecology},
