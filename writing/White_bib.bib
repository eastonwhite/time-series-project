isbn = {1471-2954 (Electronic)$\backslash$r0962-8452 (Linking)},
issn = {0962-8452},
journal = {Proceedings of the Royal Society B},
keywords = {bet-hedging,climate,dynamic optimization,fitness curve,plasticity,stochasticity},
number = {1741},
pages = {3161--3169},
pmid = {22628472},
title = {{Timing in a fluctuating environment: environmental variability and asymmetric fitness curves can lead to adaptively mismatched avian reproduction}},
volume = {279},
year = {2012}
}
