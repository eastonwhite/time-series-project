author = {Ellner, S P and Bailey, B a and Bobashev, G V and Gallant, a R and Grenfell, B T and Nychka, D W},
doi = {10.1086/286130},
file = {:Users/eastonwhite/Library/Application Support/Mendeley Desktop/Downloaded/Ellner et al. - 1998 - Noise and nonlinearity in measles epidemics combining mechanistic and statistical approaches to population modeli.pdf:pdf},
isbn = {0003-0147 (Print)$\backslash$r0003-0147 (Linking)},
issn = {0003-0147},
journal = {The American naturalist},
keywords = {analysis,be addressed,e-mail,edu,ellner,geophysical statistics project,local lyapunov exponents,measles,modeling,national center for atmospheric,ncsu,population dynamics,present address,stat,time-series,to whom correspondence should},
number = {5},
