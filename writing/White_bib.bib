doi = {10.1098/rspb.2010.2482},
file = {:Users/eastonwhite/Library/Application Support/Mendeley Desktop/Downloaded/Sheriff et al. - 2011 - Phenological variation in annual timing of hibernation and breeding in nearby populations of Arctic ground squir.pdf:pdf},
isbn = {0962-8452},
issn = {0962-8452},
