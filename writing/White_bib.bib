journal = {Ecological Applications},
keywords = {American pika,Climate change,Forbs,Graminoids,Habitat quality,Latrines,Occupancy,Ochotona princeps,Population density,Southern Rocky Mountains, USA,Vegetation quality},
number = {3},
pages = {429--435},
title = {{Determinants of pika population density vs. occupancy in the Southern Rocky Mountains}},
volume = {24},
