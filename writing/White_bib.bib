file = {:Users/eastonwhite/Library/Application Support/Mendeley Desktop/Downloaded/Chase, Knight - 2013 - Scale-dependent effect sizes of ecological drivers on biodiversity why standardised sampling is not enough.pdf:pdf},
issn = {1461-0248},
