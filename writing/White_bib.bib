issn = {1476-4687},
journal = {Nature},
keywords = {Animals,Bacteria,Bacteria: metabolism,Biological,Food Chain,Models,Nonlinear Dynamics,Oceans and Seas,Plankton,Plankton: metabolism,Population Dynamics,Species Specificity,Time Factors,chaos,density,plankton,population},
