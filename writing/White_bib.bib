author = {Knip, Danielle M and Heupel, Michelle R and Simpfendorfer, Colin A},
journal = {Marine Ecology Progress Series},
keywords = {nearshore,population model,shark},
pages = {1--11},
title = {{Sharks in nearshore environments: models, importance, and consequences}},
volume = {402},
year = {2010}
}
